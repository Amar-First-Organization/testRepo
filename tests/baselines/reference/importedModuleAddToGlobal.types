=== tests/cases/compiler/importedModuleAddToGlobal.ts ===
// Binding for an import statement in a typeref position is being added to the global scope
// Shouldn't compile b.B is not defined in C
module A {
    import b = B;
>b : typeof b
>B : typeof b

    import c = C;
>c : typeof c
>C : typeof c
}

module B {
>B : typeof B

    import a = A;
>a : any (error)
>A : any (error)

    export class B { }
>B : B
}

module C {
>C : typeof C

    import a = A;
>a : any (error)
>A : any (error)

    function hello(): b.B { return null; }
>hello : () => any (error)
>b : any (error)
>null : null
}
