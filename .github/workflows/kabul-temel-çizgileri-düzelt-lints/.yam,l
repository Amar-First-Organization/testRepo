  inşa etmek:
    çalışır: ubuntu-latest

    adımlar:
      - kullanımlar: actions/checkout@692973e3d937129bcbf40652eb9f2f61becf3332 # v4.1.7
        ile:
          belirteç: ${{ secrets.TS_BOT_GITHUB_TOKEN }}
      - kullanımlar: actions/setup-node@1e60f620b9541d16bece96c5465dc8ee9832be0b # v4.0.3
        ile:
          düğüm sürümü: 'lts/*'

      - name: Git'i Yapılandır, Testleri Çalıştır, Temel Hatları Güncelle, Düzeltmeleri Uygula
        koş: |
          git config user.email "typescriptbot@microsoft.com"
          git config user.name "TypeScript Bot"
          npm ci
          git rm -r --quiet testler/taban çizgileri/başvuru
          npx burada runtests-parallel --ci --fix || true
          npx burada temel kabul
          git add ./src
          git add ./tests/baselines/reference
          git diff --cached
          git commit -m "Temel Hatları ve/veya Uygulanan Lint Düzeltmelerini Güncelle"
          git push
